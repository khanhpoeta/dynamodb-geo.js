{
  "display": "DynamoDB Geo",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "noImplicitAny": false,
    "sourceMap": false,
    "lib": ["es2020", "dom"],
    "outDir": "dist" /* Redirect output structure to the directory. */,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["dist", "test"]
}
